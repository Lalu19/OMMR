@{
    Layout = null;
}



<!doctype html>
<html lang="en" dir="ltr">
<head>

    <!-- Meta data -->
    <meta charset="UTF-8">
    <meta name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=0'>
    <meta content="Admin Console – Business Administartion Tool By Appman Technologies" name="description">
    <meta content="Appman Technologies Private Limited" name="author">
    <meta name="keywords" content="Admin Console – Business Administartion Tool By Appman Technologies" />

    <!--favicon -->
    <link rel="icon" href="~/images/brand/Fv.png" type="image/x-icon" />

    <!-- TITLE -->
    <title>OMMR Admin Console</title>

    <!-- DASHBOARD CSS -->
    <link href="~/css/style.css" rel="stylesheet" />
    <link href="~/css/color-skins/color1.css" rel="stylesheet" />
    <link href="~/css/skins-modes.css" rel="stylesheet" />

    <!-- SINGLE-PAGE CSS -->
    <link href="~/plugins/single-page/css/single-page.css" rel="stylesheet" type="text/css">

    <!--- FONT-ICONS CSS -->
    <link href="~/css/icons.css" rel="stylesheet" />
    <style>
        .side-menu__item {
            
            color: black !important;
           
        }


        .header-brand-img {
            height: 6.5rem;
            line-height: 2rem;
            vertical-align: middle;
            margin-right: 0.5rem;
            width: auto;
        }

        #logoheight{
            height: 135px;
            margin-top: -16px;
        }

        .login100-form-title {
            font-size: 24px;
            line-height: 1.2;
            text-align: center;
            width: 100%;
            display: block;
            padding-bottom: 24px;
            color: #424344;
        }

        .signature:hover{
            color:green;
        }

       /* .ppolicy{
            padding-left:58%;
            color:green;
        }*/

        .text-left{
            padding-left: 2px;
        }

        .termss{
            color: green;
        }

        
    </style>
</head>

<body class="app dark-mode">

    <!-- BACKGROUND-IMAGE -->
    <div class="login-img">

        <!-- GLOABAL LOADER -->
        <div id="global-loader">
            <img src="~/images/svgs/loader.svg" class="loader-img" alt="Loader">
        </div>

        <div class="page h-100">
            <div class="">
                <!-- CONTAINER OPEN -->
                <div class="col col-login mx-auto">
                    
                </div>
                <div class="container-login100">
                    <div class="wrap-login100 p-6">
                        <form class="login100-form validate-form"  action="/" method="POST">
                            <div class="text-center">
                               <img src="~/images/brand/OMMR_LOGO_Final__1_-removebg-preview.png" class="header-brand-img" id="logoheight" alt="">
                            </div>   
                            <span class="login100-form-title">
                                Member Login
                            </span>
                            <div class="wrap-input100 validate-input" data-validate="Valid email is required: ex@abc.xyz">
                              
                                <input class="input100" type="text" name="username"  placeholder="Username" required>
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="zmdi zmdi-email" aria-hidden="true"></i>
                                </span>
                            </div>
                            <div class="wrap-input100 validate-input" data-validate="Password is required">
                                <input class="input100" type="password" name="password" id="typepass" placeholder="Password" required>
                                <span class="focus-input100"></span>
                                <span class="symbol-input100">
                                    <i class="zmdi zmdi-lock" aria-hidden="true"></i>
                                </span>
                            </div>

                            <div class="text-left pt-1">
                                <p class="mb-0"><input type="checkbox" onclick="Toggle()"><a class="text-primary ml-1">Show Password</a></p>
                            </div>
                            <div class="container-login100-form-btn">
                                <button type="submit" class="login100-form-btn btn-primary">Sign In</button> 
                            </div>
                            <div class=" flex-c-m text-center mt-3">
                                <div class="login-logo">
                                    <div class="alert alert-danger" role="alert" style="display: table-row-group;">
	                                  @ViewBag.ErrorMessage
	                                </div>
                                </div>
                            </div>
                            <div>
                                <span class="backto-link font-default content-color text-decoration-none">Don't have an account ? <a class="text-decoration-underline theme-color signn" href="/Home/SignUpForClient"> Signup  </a> </span>
                            </div>
                        </form>


                    </div>
                </div>
                <!-- CONTAINER CLOSED -->
             @*   <div style="padding-left: 20px;">
                    Designed & Developed by <a href="https://appman.in/" class="signature">Appman Technologies Pvt.Ltd</a> <a class="ppolicy" href="/PrivacyPolicy/PrivacyPolicy">Privacy Policy</a> & <a class="termss" href="/PrivacyPolicy/TermsandCondition">Terms and Condition</a>
               
                </div>*@
                <div class="container">
                    <div class="row">
                        <div class="col-md-9">
                            Designed & Developed by <a href="https://appman.in/" class="signature">Appman Technologies Pvt.Ltd</a>
                        </div>
                        <div class="col-md-3" style="padding-left: 76px;">
                            <a class="ppolicy" href="/PrivacyPolicy/PrivacyPolicy">Privacy Policy</a> &
                            <a class="termss" href="/PrivacyPolicy/TermsandCondition">Terms and Condition</a>
                        </div>
                    </div>
                </div>

        </div>
    </div>

    <!-- BACKGROUND-IMAGE CLOSED -->
    <!-- JQUERY SCRIPTS JS-->
    <script src="~/js/vendors/jquery-3.2.1.min.js"></script>

    <!-- BOOTSTRAP SCRIPTS JS-->
    <script src="~/js/vendors/bootstrap.bundle.min.js"></script>

    <!-- SPARKLINE JS-->
    <script src="~/js/vendors/jquery.sparkline.min.js"></script>

    <!-- CHART-CIRCLE JS-->
    <script src="~/js/vendors/circle-progress.min.js"></script>

    <!-- RATING STAR JS-->
    <script src="~/plugins/rating/rating-stars.js"></script>

    <!-- INPUT MASK JS-->
    <script src="~/plugins/input-mask/input-mask.min.js"></script>

    <!-- CUSTOM JS-->
    <script src="~/js/custom.js"></script>
    <script>
        function Toggle() {
            var temp = document.getElementById("typepass");
            if (temp.type === "password") {
                temp.type = "text";
            }
            else {
                temp.type = "password";
            }
        }
    </script>
</body>
</html>

